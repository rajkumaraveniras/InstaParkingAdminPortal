                alert('Please upload Lot1 Image')
            }
            else {
                $('#Lotupload2').click();
            }
        })
        $('#LotImg3').click(function () {
            if ($("#Lotupload2").val() == "" && $('#LotImg2').val() == "") {
                alert('Please upload Lot2 Image')

            } else {
                $('#Lotupload3').click();
            }
        })




        $('#Lotupload').change(function (event) {
            if (event.target.files.length > 0) {
                var filetype = event.target.files[0].name.split('.')[event.target.files[0].name.split('.').length - 1]

                if (filetype != 'jpg' && filetype != 'jpeg' && filetype != 'png') {
                    alert('Please Select valid images only')
                    $("#Lotupload").val("");
                } else {
                    var tmppath = URL.createObjectURL(event.target.files[0]);
                    $('#LotImg').attr('src', tmppath)
                }
            }
        });
        $('#Lotupload2').change(function (event) {
